<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.anshit.survey" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/survey_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Login"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Signup" android:screenOrientation="portrait"/>
        <activity android:name=".MainActivity" android:screenOrientation="portrait"/>
        <activity android:name=".DisplayNewSurvey" android:screenOrientation="portrait" android:parentActivityName=".MainActivity"/>
        <activity android:name=".DisplayFilledSurvey" android:screenOrientation="portrait" android:parentActivityName=".MainActivity"/>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>
